<div id="blogList">
  {@|apply blog-filter.block}
<div class="content">

  {.repeated section items}

  <article class="{@|item-classes}" id="article-{id}" data-item-id="{id}">

    <section class="blogMain">
      <header class="">
        {.if title}
        <h1 class="entry-title">
          {.passthrough?}
            <a href="{sourceUrl}">{title} <span>&rarr;</span></a>
          {.or}
            <a href="{fullUrl}">{title}</a>
          {.end}
        </h1>
        {.end}
      </header>
      {.if assetUrl}
      <img src="{assetUrl}">
      {.or}
      <script>
      var bodyObj = {body|json-pretty}
      var dataSrcStartRegex = /data-src="/
      var dataSrcEndRegex = /" data-image=/
      var dataSrcStartIndex = bodyObj.search(dataSrcStartRegex)
      var dataSrcEndIndex = bodyObj.search(dataSrcEndRegex)
      var imgSrc = bodyObj.substring(dataSrcStartIndex + 10, dataSrcEndIndex)
      console.log(imgSrc)
      </script>
      <img src="{imgSrc}">
      {.end}

      <div class="sqs-post-entry">{postItemInjectCode}</div>

    </section>

   </article>

  {.end}
  <!--PAGINATION-->

  {.section pagination}
    <nav class="pagination">
        {.if pagination.prevPage}<a href="{prevPageUrl}">Newer</a>{.or}Newer{.end} /
        {.if pagination.nextPage}<a href="{nextPageUrl}">Older</a>{.or}Older{.end}
    </nav>
  {.end}
</div>
</div>
